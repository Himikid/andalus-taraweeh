+        trial_dir=args.trial_dir,
+        repo_root=args.repo_root.resolve(),
+    )
+
+    output_path = args.trial_dir / f"stage-{args.stage}.json"
+    _write_json(output_path, result)
+    _log(f"saved stage result: {output_path}")
+
+
+if __name__ == "__main__":
+    main()
