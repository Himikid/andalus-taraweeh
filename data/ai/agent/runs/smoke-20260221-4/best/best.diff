+    stage_score = round(fmean(item["score"]["score"] for item in dataset_results), 4) if dataset_results else -999.0
+
+    return {
+        "stage": stage_name,
+        "strategy_id": strategy.get("id"),
+        "stage_score": stage_score,
+        "datasets": dataset_results,
+    }
